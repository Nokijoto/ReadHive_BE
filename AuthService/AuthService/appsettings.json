{
  "AppEnvironment": "${APP_ENVIRONMENT}",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "SecretKey": "${JWT_SECRET_KEY}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=${AUTH_SERVER};Database=${AUTH_DB};User Id=${AUTH_DB_USER_ID};Password=${AUTH_DB_PASSWORD};TrustServerCertificate=${AUTH_DB_TRUST_SERVER_CERTIFICATE};"

  },
  "MailSettings": {
    "SmtpHost": "${SMTP_HOST}",
    "SmtpPort": "${SMTP_PORT}",
    "Username": "${SMTP_USERNAME}",
    "Password": "${SMTP_PASSWORD}",
    "From": "${FROM}"
  },
  "AppSettings": {
    "ClientUrl": "http://localhost:5186"
  } 
}
